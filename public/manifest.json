{
  "name": "Fontcookie",
  "description": "CHOMCO COOKY",
  "version": "1.0",
  "manifest_version": 3,
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["fontcookie.js"],
      "run_at": "document_start"
    }
  ],
  "permissions": ["tabs", "storage"],
  "host_permissions": [
  ],
  "action": {
    "default_popup": "index.html"
  },
  "icons": {
    "128": "fontcookie.png"
  }
}
